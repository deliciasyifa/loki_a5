        </div>
      </div>
      <!--**********************************
            Nav header end
        ***********************************-->

      <!--**********************************
            Header start
        ***********************************-->
      <div class="header" style="background-color: #10a395">
        <div class="header-content clearfix">
          <div class="nav-control"></div>
          <div class="header-right">
            <a class="btn btn-list" href="login.html" style="box-shadow: 0px 4px 4px 5px rgba(0, 0, 0, 0.1); position: relative; top: 20px">Login</a>
          </div>
        </div>
      </div>
      <!--**********************************
            Header end ti-comment-alt
        ***********************************-->

      <!--**********************************
            Sidebar start
        ***********************************-->

      <!--**********************************
            Sidebar end
        ***********************************-->

      <!--**********************************
            Content body start
        ***********************************-->
      <div>
        <div class="content-body ">
          <div class="container-fluid mt-6">
            <div class="card-body">
              <h3 style="text-align: center">Rencana Pembelajaran Semester</h3>
              <div class="row">
                <div class="col-12" style="margin-top: 10px">
                  <div class="card2 mb-4" style="border-radius: 10px">
                    <div class="card-body" style="box-shadow: 0px 4px 4px 3px rgba(0, 0, 0, 0.25); border-radius: 10px">
                      <div class="container-fluid">
                        <div class="row">
                          <table id="example" class="table table-striped" style="width: 100%">
                            <thead>
                              <tr>
                                <th>Kode Mata Kuliah</th>
                                <th>Nama Mata Kuliah</th>
                                <th>Aksi</th>
                              </tr>
                            </thead>
                            <tbody>
                              <% for(var i=0; i < data.length; i++ ) { %>
                                <tr>
                                  <td><%= data[i].code %></td>
                                  <td><%= data[i].name %></td>
                                  <td>
                                    <button type="button" class="btn btn-primary">Lihat</button>
                                  </td>
                                </tr>
                                <% } %>
                              
                            </tfoot>
                          </table>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>

      <!--**********************************
              Content body end
          ***********************************-->

      <!--**********************************
            Footer start
        ***********************************-->

      <div class="footer">
        <div class="copyright">
